    <AnchorPane>
        <VBox spacing="30">
            <BorderPane fx:id="header"/>
            <VBox>
                <padding>
                    <Insets top="30" left="30.0" right="30.0"/>
                </padding>
                <Pane>
                    <padding>
                        <Insets left="30"/>
                    </padding>
                    <Text style="-fx-font-family: System; -fx-font-size: 25;" text="Nos produits" />
                </Pane>
                <GridPane fx:id="gridDisplay" hgap="30.0" vgap="30.0" minHeight="690.0" minWidth="920.0" style="-fx-background-color: #dddddd;">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="100.0" prefWidth="100.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="100.0" prefWidth="100.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="100.0" prefWidth="100.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="100.0" prefWidth="100.0"/>
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="100.0" prefHeight="100.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="100.0" prefHeight="100.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="100.0" prefHeight="100.0" vgrow="SOMETIMES"/>
                    </rowConstraints>
                    <padding>
                        <Insets bottom="15.0" left="15.0" right="15.0" top="15.0"/>
                    </padding>
                </GridPane>
            </VBox>
            <BorderPane fx:id="contact"/>
        </VBox>
    </AnchorPane>
</ScrollPane>
